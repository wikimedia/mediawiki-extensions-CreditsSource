{
	"name": "CreditsSource",
	"version": "0.4.0",
	"author": [
		"Hans Musil",
		"Matthias Mullie"
	],
	"license-name": "GPL-2.0-only",
	"url": "https://www.mediawiki.org/wiki/Extension:CreditsSource",
	"descriptionmsg": "creditssource-desc",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.45"
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": "schema"
	},
	"HookHandlers": {
		"schema": {
			"class": "MediaWiki\\Extension\\CreditSource\\SchemaHooks"
		}
	},
	"MessagesDirs": {
		"CreditsSource": [
			"i18n"
		]
	},
	"Actions": {
		"credits": "MediaWiki\\Extension\\CreditSource\\CreditsSourceAction"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\CreditSource\\": "includes/"
	},
	"manifest_version": 2
}
